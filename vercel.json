{
  "version": 2,
  "builds": [
    { "src": "api/ai.js", "use": "@vercel/node" },
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "styles.css", "use": "@vercel/static" },
    { "src": "app.js", "use": "@vercel/static" },
    { "src": "modules/(.*)", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/ai", "dest": "/api/ai.js" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}